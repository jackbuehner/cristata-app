{
  "editor.formatOnSave": true,
  "editor.fontFamily": "'Dank Mono', Consolas, 'Courier New', monospace",
  "editor.tabSize": 2,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.tabs.defaultColor": "terminal.ansiCyan",
  "terminal.integrated.tabs.defaultIcon": "organization",
  "typescript.preferences.autoImportFileExcludePatterns": ["src/lib/**/[_]*.*"]
}
