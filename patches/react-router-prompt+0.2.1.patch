ion();\n        tx.retry();\n      }\n    },\n    [resetConfirmation, onConfirm]\n  );\n\n  useBlocker(blocker, when && !hasConfirmed);\n\n  return (\n    <div>\n      {children({\n        isActive,\n        onConfirm: proceed,\n        onCancel: cancel,\n      })}\n    </div>\n  );\n};\n\nexport default ReactRouterPrompt;\n"],"names":["noop","initialConfirmState","isActive","hasConfirmed","proceed","cancel","useConfirm","useState","confirm","setConfirm","useEffect","window","onbeforeunload","onConfirm","useCallback","promise","Promise","resolve","reject","prevState","then","resetConfirmation","useBlocker","blocker","when","useContext","NavigationContext","navigator","unblock","block","tx","autoUnblockingTx","retry","ReactRouterPrompt","children","React","onCancel"],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAEA,IAAMA,IAAI,GAAG,SAAPA,IAAO;;AAEZ,CAFD;;AAIA,IAAMC,mBAAmB,GAAG;EAC1BC,QAAQ,EAAE,KADgB;EAE1BC,YAAY,EAAE,KAFY;EAG1BC,OAAO,EAAEJ,IAHiB;EAI1BK,MAAM,EAAEL;AAJkB,CAA5B;;AAmBA,IAAMM,UAAU,GAAG,SAAbA,UAAa;EACjB,gBAA8BC,cAAQ,CAAmBN,mBAAnB,CAAtC;MAAOO,OAAP;MAAgBC,UAAhB;;EAEAC,eAAS,CAAC;IACR,IAAIF,OAAO,CAACN,QAAZ,EAAsB;MACpBS,MAAM,CAACC,cAAP,GAAwB;QACtB,OAAO,KAAP;OADF;;;IAKF,OAAO;MACL,IAAIJ,OAAO,CAACN,QAAZ,EAAsBS,MAAM,CAACC,cAAP,GAAwB,IAAxB;KADxB;GAPO,EAUN,CAACJ,OAAD,CAVM,CAAT;EAYA,IAAMK,SAAS,GAAGC,iBAAW,CAAC;IAC5B,IAAMC,OAAO,GAAG,IAAIC,OAAJ,CAAY,UAACC,OAAD,EAAUC,MAAV;MAC1BT,UAAU,CAAC,UAACU,SAAD;QAAA,oBACNA,SADM;UAETjB,QAAQ,EAAE,IAFD;UAGTE,OAAO,EAAEa,OAHA;UAITZ,MAAM,EAAEa;;OAJA,CAAV;KADc,CAAhB;IASA,OAAOH,OAAO,CAACK,IAAR,CACL;MACEX,UAAU,cAAMD,OAAN;QAAeN,QAAQ,EAAE,KAAzB;QAAgCC,YAAY,EAAE;SAAxD;MACA,OAAO